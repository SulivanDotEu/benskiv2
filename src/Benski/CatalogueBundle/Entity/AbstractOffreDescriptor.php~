<?php

namespace Benski\CatalogueBundle\Entity;

use Doctrine\ORM\Mapping as ORM;
use Benski\CommonBundle\Entity\VersionedObject;

/**
 * VersionedObject
 *
 * @ORM\MappedSuperclass
 */
abstract class AbstractOffreDescriptor extends VersionedObject
{

    /**
     * @var integer
     *
     * @ORM\Column(name="qualite", type="smallint")
     */
    private $qualite;

    
    /**
     * @var \DateTime
     *
     * @ORM\Column(name="dateVerouillage", type="datetime")
     */
    private $dateVerouillage;
    
    // options
    
    

    
    public function getObjetReserve(){}


    
}
