<?php

namespace Benski\CatalogueBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * PackDescriptor
 *
 * @ORM\Table()
 * @ORM\Entity(repositoryClass="Benski\CatalogueBundle\Entity\PackDescriptorRepository")
 */
class PackDescriptor extends AbstractOffreDescriptor {

   /**
    * @var integer
    *
    * @ORM\Column(name="id", type="integer")
    * @ORM\Id
    * @ORM\GeneratedValue(strategy="AUTO")
    */
   private $id;

   /**
    * @var \stdClass
    *
    * @ORM\ManyToOne(targetEntity="Benski\CatalogueBundle\Entity\Date")
    */
   private $date;

   /**
    * @var \stdClass
    *
    * @ORM\ManyToOne(targetEntity="Benski\CatalogueBundle\Entity\Destination")
    */
   private $destination;

   /**
    * @var integer
    *
    * @ORM\Column(name="nombrePlaces", type="smallint")
    */
   private $nombrePlaces;

   /**
    * @var float
    *
    * @ORM\Column(name="prixEnVigueur", type="decimal")
    */
   private $prixEnVigueur;
   
   /**
    * @var float
    *
    * @ORM\Column(name="prixDeBase", type="decimal")
    */
   private $prixDeBase;
   
    /**
     * @var float
     *
     * @ORM\Column(name="prixLitVide", type="decimal")
     */
    private $prixLitVide;






}