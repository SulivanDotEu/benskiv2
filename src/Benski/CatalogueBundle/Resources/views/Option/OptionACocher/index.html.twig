{% extends 'BenskiCatalogueBundle:Option/OptionACocher:layout.html.twig' %}

{% block body -%}
    <h1>Option\OptionACocher list</h1>

    <table class="table table-striped">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>Description</th>
                <th>Type</th>
                <th>Explication</th>
                <th>Version</th>
                <th>Published</th>
                <th>Intitule</th>
                <th>Pardefault</th>
                <th>Inforequis</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('option_optionacocher_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.nom }}</td>
                <td>{{ entity.description }}</td>
            <td>{{ ("option.type."~entity.type)|trans }}</td>
                <td>{{ entity.explication }}</td>
                <td>{{ entity.version }}</td>
                <td>{{ entity.published }}</td>
                <td>{{ entity.intitule }}</td>
                <td>{{ entity.parDefault }}</td>
                <td>{{ entity.infoRequis }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('option_optionacocher_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('option_optionacocher_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('option_optionacocher_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
