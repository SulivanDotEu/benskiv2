{% extends 'BenskiCatalogueBundle:OptionChoixMultiple:layout.html.twig' %}

{% block body -%}
    <h1>OptionChoixMultiple list</h1>

    <table class="table table-striped">
        <thead>
            <tr>
                <th>Nom</th>
                <th>Description</th>
                <th>Type</th>
                <th>Explication</th>
                <th>Id</th>
                <th>Choixpardefaut</th>
                <th>Version</th>
                <th>Published</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('optionchoixmultiple_show', { 'id': entity.id }) }}">{{ entity.nom }}</a></td>
                <td>{{ entity.description }}</td>
                <td>{{ entity.type }}</td>
                <td>{{ entity.explication }}</td>
                <td>{{ entity.id }}</td>
                <td>{{ entity.choixParDefaut }}</td>
                <td>{{ entity.version }}</td>
                <td>{{ entity.published }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('optionchoixmultiple_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('optionchoixmultiple_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('optionchoixmultiple_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
