{% extends 'BenskiReservationBundle:AppartementReserve:layout.html.twig' %}

{% block body -%}
    <h1>AppartementReserve</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Numero</th>
                <td>{{ entity.numero }}</td>
            </tr>
            <tr>
                <th>Nombrepersonnes</th>
                <td>{{ entity.nombrePersonnes }}</td>
            </tr>
            <tr>
