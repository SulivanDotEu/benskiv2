{% extends  'WalvaCrudAdminBundle:Crud:show.html.twig' %}

{% block crud_table_body -%}
    <tr>
        <th>Id</th>
        <td>{{ entity.id }}</td>
    </tr>
    <tr>
        <th>Montant</th>
        <td>{{ entity.montant }}</td>
    </tr>
    <tr>
        <th>Description</th>
        <td>{{ entity.description }}</td>
    </tr>
    <tr>
        <th>Reservation</th>
        <td>
            <a class="btn btn-primary btn-xs" href="{{ path('admin_reservation') }}">
                {{ entity.reservation }}
            </a>
        </td>
    </tr>
{% endblock %}
