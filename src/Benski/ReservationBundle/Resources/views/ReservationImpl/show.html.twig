{% extends 'BenskiReservationBundle:ReservationImpl:layout.html.twig' %}

{% block body -%}
<h1>ReservationImpl</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Responsable</th>
            <td>{{ entity.responsable }}</td>
        </tr>
        <tr>
            <th>Total</th>
            <td>{{ entity.total }}</td>
        </tr>
        <tr>
            <th>Statut</th>
            <td>{{ entity.statut }}</td>
        </tr>
        <tr>
            <th>Datecreation</th>
            <td>{{ entity.dateCreation|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Datemodification</th>
            <td>{{ entity.dateModification|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Reductions</th>
            <td>{{ entity.reductions }}</td>
        </tr>
        <tr>
            <th>Paiements</th>
            <td>{{ entity.paiements }}</td>
        </tr>
    </tbody>
</table>



<ul class="record_actions">
    <li>
        <a href="{{ path('admin_reservation') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('admin_reservation_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
